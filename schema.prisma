// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id       String  @id @default(cuid())
  name     String  @default("")
  email    String  @unique @default("")
  role     String?
  password String
}

model Color {
  id                  String    @id @default(cuid())
  name                String    @unique @default("")
  red                 Int
  green               Int
  blue                Int
  hex                 String    @unique @default("")
  from_Palette_colors Palette[] @relation("Palette_colors")
}

model Palette {
  id     String  @id @default(cuid())
  name   String  @default("")
  colors Color[] @relation("Palette_colors")
}

model Frame {
  id     String @id @default(cuid())
  name   String @unique @default("")
  width  Int
  height Int
}

model Presale {
  id   String @id @default(cuid())
  name String @default("")
}

model Sale {
  id   String @id @default(cuid())
  name String @default("")
}